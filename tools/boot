cd ~
rm -rf fair
id=fair
f=Fair-1.tar.gz
mkdir $id && cd $id && curl https://fairlayer.com/$f -o $f
tar -xzf $f && rm $f
cp -r ~/work/fair/node_modules node_modules

rm -rf ./src
rm -rf ./wallet
cp -r ~/work/fair/wallet wallet
cp -r ~/work/fair/src src
node fair -p8001 --nocrypto --synclimit=100000 --stop_blocks=100000 $1